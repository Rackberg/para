{
    "name": "lrackwitz/para",
    "description": "Commandline Application to execute shell commands in multiple folders parallel at the same time.",
    "license": "GPLv3",
    "require": {
        "symfony/console": ">=2.9 ^3.4",
        "symfony/process": ">=2.9 ^3.4",
        "symfony/monolog-bundle": ">=2.9 ^3.0",
        "symfony/yaml": ">=2.9 ^3.4",
        "monolog/monolog": "^1.22"
    },
    "authors": [
        {
            "name": "Lars Rackwitz-Rosenberg",
            "email": "rackwitz.lars@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "lrackwitz\\Para\\": "src/",
            "lrackwitz\\Para\\tests\\": "tests/"
        }
    },
    "bin": [
        "bin/para"
    ],
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "mikey179/vfsStream": "^1.6"
    }
}
